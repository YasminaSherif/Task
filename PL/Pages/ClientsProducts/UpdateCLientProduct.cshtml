@page
@model PL.Pages.ClientsProducts.UpdateCLientProductModel
@{
}


<h1>Edit ClientProduct</h1>

<form method="post">
    <div class="form-group">
        <label for="ClientId">Client Id</label>
        <input type="text" class="form-control" id="ClientId" asp-for="ClientProduct.ClientId" required />
        <span asp-validation-for="ClientProduct.ClientId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="ProductId">Product Id</label>
        <input type="text" class="form-control" id="ProductId" asp-for="ClientProduct.ProductId" required />
        <span asp-validation-for="ClientProduct.ProductId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="EndDate">End Date</label>
        <input type="date" class="form-control" id="EndDate" asp-for="ClientProduct.EndDate" />
        <span asp-validation-for="ClientProduct.EndDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="StartDate">Start Date</label>
        <input type="date" class="form-control" id="StartDate" asp-for="ClientProduct.StartDate" required />
        <span asp-validation-for="ClientProduct.StartDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="License">License</label>
        <input type="text" class="form-control" id="License" asp-for="ClientProduct.License" required />
        <span asp-validation-for="ClientProduct.License" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>

<partial name="_ValidationScriptsPartial" />
