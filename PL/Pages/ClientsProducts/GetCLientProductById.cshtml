@page
@model PL.Pages.ClientsProducts.GetCLientProductByIdModel
@{
}



<h1>ClientProduct Details</h1>

<div>
  
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Client Id</dt>
        <dd class="col-sm-10">@Model.ClientProduct.ClientId</dd>
        <dt class="col-sm-2">Product Id</dt>
        <dd class="col-sm-10">@Model.ClientProduct.ProductId</dd>
        <dt class="col-sm-2">License</dt>
        <dd class="col-sm-10">@Model.ClientProduct.License</dd>
        <dt class="col-sm-2">Start Date</dt>
        <dd class="col-sm-10">@Model.ClientProduct.StartDate</dd>
        <dt class="col-sm-2">End Date</dt>
        <dd class="col-sm-10">@Model.ClientProduct.EndDate</dd>
    </dl>
</div>


<a asp-page="clientsProducts" class="btn btn-primary">Back to List</a>
<a asp-page="UpdateClientProduct" asp-route-id="@Model.ClientProduct.Id" class="btn btn-secondary">Edit</a>
